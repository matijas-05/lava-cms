generator client {
    provider        = "prisma-client-js"
    previewFeatures = ["multiSchema"]
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
    schemas  = ["admin", "frontend"]
}

model users {
    id   Int    @id @default(autoincrement())
    name String

    @@schema("admin")
}
