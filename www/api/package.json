{
	"name": "api",
	"version": "0.1.0",
	"private": true,
	"main": "main.ts",
	"type": "module",
	"scripts": {
		"start": "NODE_ENV=production tsx .",
		"dev": "NODE_ENV=development tsx watch --clear-screen=false .",
		"lint": "tsc && eslint .",
		"db:push": "prisma db push && dotenv -e .env.test -- prisma db push",
		"test": "vitest run",
		"test:dev": "vitest",
		"test:ui": "vitest --ui",
		"test:coverage": "vitest --coverage",
		"test:report": "vite preview --base /__vitest__ --outDir html --open"
	},
	"dependencies": {
		"@mswjs/http-middleware": "0.6.0",
		"@prisma/client": "^4.13.0",
		"@tsconfig/node18-strictest-esm": "^1.0.1",
		"bcrypt": "^5.1.0",
		"cors": "^2.8.5",
		"dotenv": "^16.0.3",
		"express": "^4.18.2",
		"msw": "0.49.3",
		"strict-event-emitter": "0.3.2",
		"trpc-panel": "^1.3.4",
		"tsx": "^3.12.3",
		"zod": "^3.20.2"
	},
	"devDependencies": {
		"@types/bcrypt": "^5.0.0",
		"@types/cors": "^2.8.13",
		"@types/express": "^4.17.17",
		"@types/node": "^18.16.1",
		"@types/supertest": "^2.0.12",
		"@vitest/coverage-c8": "0.28.3",
		"@vitest/ui": "0.28.3",
		"dotenv-cli": "^7.2.1",
		"headers-polyfill": "^3.1.2",
		"prisma": "^4.13.0",
		"supertest": "^6.3.3",
		"vite": "4.2.2",
		"vitest": "0.28.3",
		"vitest-mock-extended": "1.0.9"
	}
}
