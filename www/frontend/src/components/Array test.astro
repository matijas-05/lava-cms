---
interface Props {
	cms: {
		header: string;
		articles: boolean[];
	};
}

const { cms } = Astro.props;
---

<div>
	<h1>{cms.header}</h1>
	<div class="articles">
		{cms.articles.map((article) => <p>{String(article)}</p>)}
	</div>
</div>

<style>
	.articles {
		border: 1px solid black;
	}
</style>
