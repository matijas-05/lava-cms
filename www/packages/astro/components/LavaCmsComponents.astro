---
import type { CmsComponent } from "@lavacms/types";
import LavaCmsComponent from "./LavaCmsComponent.astro";

interface Props {
	components: CmsComponent[];
}

const { components: cmsComponents } = Astro.props;
if (!cmsComponents) {
	throw new Error("No components provided to `LavaCmsComponents` Astro component");
}
---

{cmsComponents.map((cmsComp) => <LavaCmsComponent component={cmsComp} />)}
